{
  "version": "0.2.0",
  "configurations": [
    {
        "name": "Launch",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/node_modules/webpack-dev-server/bin/webpack-dev-server",
        "args": [
            "-d",
            "--config",
            "internals/build/webpack.dev.conf.js"
        ],
        "outFiles": [
            "${workspaceRoot}/dist/*"
        ],
        "stopOnEntry": false,
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": null,
        "runtimeArgs": [],
        "env": {
            "NODE_ENV": "development"
        },
        "console": "internalConsole",
        "sourceMaps": true
    }
  ]
}